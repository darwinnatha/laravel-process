{
  "name": "darwinnatha/laravel-process",
  "description": "Un micro-package Laravel basé sur le pattern pipeline-task pour les processus métier séquentiels.",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Darwin Nathan",
      "email": "fotsodarwin@gmail.com"
    }
  ],
  "require": {
    "php": "^8.0",
    "illuminate/support": "^8.0|^9.0|^10.0|^11.0|^12.0",
    "illuminate/contracts": "*",
    "illuminate/http": "^8.0|^9.0|^10.0|^11.0|^12.0",
    "illuminate/console": "^8.0|^9.0|^10.0|^11.0|^12.0"
  },
  "require-dev": {
    "mockery/mockery": "^1.6",
    "orchestra/testbench": "*",
    "pestphp/pest": "*"
  },
  "config": {
    "sort-packages": true,
    "allow-plugins": {
      "pestphp/pest-plugin": true
    }
  },
  "scripts": {
    "test": [
      "pest"
    ]
  },
  "autoload": {
    "psr-4": {
      "DarwinNatha\\Process\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "DarwinNatha\\Process\\Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "DarwinNatha\\Process\\ProcessServiceProvider"
      ]
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
